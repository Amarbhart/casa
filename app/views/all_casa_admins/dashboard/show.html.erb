<div class="row">
  <div class="col-sm-12 dashboard-table-header">
    <h1>All CASA Admin</h1>
  </div>
</div>
C
<div class="row">
  <div class="col-sm-12 dashboard-table-header">
    <h2>CASA Organizations</h2>
    <%= link_to "New CASA Organization", new_all_casa_admins_casa_org_path, class: "btn btn-primary" %>
  </div>
</div>

<table class="table organization-list">
  <thead>
    <tr>
      <th>Name</th>
      <th>Created At</th>
    </tr>
  </thead>
  <tbody>
    <% @casa_orgs.each do |casa_org| %>
      <tr>
        <td><%= link_to casa_org.name, all_casa_admins_casa_org_path(casa_org) %></td>
        <td><%= casa_org.created_at.strftime('%B %e, %Y') %></td>
      </tr>
    <% end %>
  </tbody>
</table>
