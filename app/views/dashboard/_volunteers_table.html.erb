<table class="table table-striped table-bordered" id="volunteers">
  <thead>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Supervisor</th>
    <th>Status</th>
    <th>Assigned To Transition Aged Youth</th>
    <th>Case Number(s)</th>
    <th>Last Contact Made</th>
    <th>Contacts Made in Past 60 Days</th>
    <th>Actions</th>
  </tr>
  </thead>

  <tbody>
# frozen_string_literal: true
@volunteers.each
 do |volunteer| %>
    <tr>
# frozen_string_literal: true
link_to(volunteer.decorate.name, edit_volunteer_path(volunteer))
 %></td>
# frozen_string_literal: true
volunteer.email
 %></td>
      <td id="supervisor-column">
# frozen_string_literal: true
volunteer.supervisor.nil? ? '' : link_to(volunteer.supervisor.decorate.name, edit_supervisor_path(volunteer.supervisor))
 %>
      </td>
# frozen_string_literal: true
volunteer.status
 %></td>
# frozen_string_literal: true
volunteer&.assigned_to_transition_aged_youth?
 %></td>
# frozen_string_literal: true
safe_join(volunteer&.casa_cases&.map { |c| link_to(c.case_number, c) }, ', ')
 %></td>
# frozen_string_literal: true
link_to_if volunteer&.most_recent_contact.present?, volunteer.last_contact_made, volunteer&.most_recent_contact&.casa_case
 %></td>
# frozen_string_literal: true
volunteer&.recent_contacts_made
 %></td>
# frozen_string_literal: true
link_to 'Edit', edit_volunteer_path(volunteer)
 %></td>
    </tr>
  <% end %>
  </tbody>
</table>
